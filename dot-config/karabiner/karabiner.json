{
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "Change spacebar key to command+control+option+shift if pressed with other keys",
                        "enabled": false,
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "spacebar",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "key_code": "left_shift",
                                        "modifiers": ["left_command", "left_control", "left_option"]
                                    }
                                ],
                                "to_if_alone": [
                                    {
                                        "hold_down_milliseconds": 500,
                                        "key_code": "spacebar"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Middle Mouse Button to as Look Up & Data Detector (3-finger tap)",
                        "manipulators": [
                            {
                                "from": {
                                    "modifiers": { "optional": ["any"] },
                                    "pointing_button": "button3"
                                },
                                "to": [
                                    {
                                        "key_code": "d",
                                        "modifiers": ["left_command", "left_control"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "F4 to CTRL+Space",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "f4",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "key_code": "spacebar",
                                        "modifiers": ["left_control"],
                                        "repeat": true
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Change caps_lock to control if pressed with other keys. (rev 2)",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "caps_lock",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "left_control" }],
                                "to_if_alone": [
                                    {
                                        "hold_down_milliseconds": 500,
                                        "key_code": "caps_lock"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Change left_shift to alt/option+b if pressed alone in tmux/iTerm",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "com\\.googlecode\\.iterm2",
                                            ""
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": {
                                    "key_code": "left_shift",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "left_shift" }],
                                "to_if_alone": [
                                    {
                                        "key_code": "b",
                                        "modifiers": ["left_alt"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
            "devices": [
